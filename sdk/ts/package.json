{
  "name": "@llm-dev-ops/infra",
  "version": "0.1.0",
  "description": "TypeScript SDK for LLM-Dev-Ops Infrastructure Layer",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./crypto": {
      "import": "./dist/crypto/index.js",
      "types": "./dist/crypto/index.d.ts"
    },
    "./id": {
      "import": "./dist/id/index.js",
      "types": "./dist/id/index.d.ts"
    },
    "./json": {
      "import": "./dist/json/index.js",
      "types": "./dist/json/index.d.ts"
    },
    "./llm-client": {
      "import": "./dist/llm-client/index.js",
      "types": "./dist/llm-client/index.d.ts"
    },
    "./retry": {
      "import": "./dist/retry/index.js",
      "types": "./dist/retry/index.d.ts"
    },
    "./cache": {
      "import": "./dist/cache/index.js",
      "types": "./dist/cache/index.d.ts"
    },
    "./rate-limit": {
      "import": "./dist/rate-limit/index.js",
      "types": "./dist/rate-limit/index.d.ts"
    }
  },
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "build": "tsc",
    "build:wasm": "cd ../../crates && wasm-pack build --target web",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "llm",
    "infrastructure",
    "crypto",
    "vector",
    "wasm"
  ],
  "author": "LLM-Dev-Ops",
  "license": "LicenseRef-LLM-Dev-Ops",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": ">=0.20.0"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    }
  }
}
